{
  "buildCommand": "cd archon-ui-main && npm run build",
  "outputDirectory": "archon-ui-main/dist",
  "installCommand": "cd archon-ui-main && npm install",
  "functions": {
    "python/api/**.py": {
      "runtime": "@vercel/python@4.1.0"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/python/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}