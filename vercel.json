{
  "buildCommand": "cd archon-ui-main && npm ci && npm run build",
  "outputDirectory": "archon-ui-main/dist",
  "installCommand": "cd archon-ui-main && npm ci",
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/index.py"
    },
    {
      "source": "/api/sparc/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}